{
  "version": 2,
  "buildCommand": "./vercel-build.sh",
  "outputDirectory": "portfolio-app/build",
  "installCommand": "cd portfolio-app && npm install --legacy-peer-deps --force",
  "framework": "create-react-app",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
